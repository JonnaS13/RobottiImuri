
public class robo {

	public static void main(String[] args) {
		//muodostetaan RobottiImuriluokasta olio eli instance
		RobottiImuri robottiImuri1 = new RobottiImuri();
		
		robottiImuri1.merkki = "roborock";
		robottiImuri1.malli = "s6maxV";
		robottiImuri1.akunkesto = 210;
		robottiImuri1.kaynnisty();
		robottiImuri1.tulostaTiedot();
		robottiImuri1.imuroi();
		robottiImuri1.palaaLaturiin();

	}

} // Main lopetus

class RobottiImuri 
		{
		//ominaisuudet
		String merkki;
		String malli;
		int akunkesto;
		
		//toiminnallisuudet
		public void kaynnisty()
		{
			System.out.println("Robotti-imuri p채채lle");
		
		}
		
		public void imuroi()
		{
			System.out.println("Aloittaa imuroinnin");
		}
		
		public void palaaLaturiin()
		{
			System.out.println("Imurointi on p채채ttynyt");
		}
		public void tulostaTiedot()
		{
			System.out.printf("Imurin tiedot: " + merkki +" " + malli);
		}
	
	
	
	}

